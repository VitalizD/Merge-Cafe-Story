      - Character: 0
        Emotion: 0
        Phrase: "\u041F\u043E\u0447\u0435\u043C\u0443 \u0442\u044B \u043D\u0435 \u043C\u043E\u0436\u0435\u0448\u044C
          \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F \u0434\u043E\u043C\u043E\u0439?"
      - Character: 3
        Emotion: 0
        Phrase: "\u0414\u0435\u043B\u043E \u0432 \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u044F\u0445."
      - Character: 3
        Emotion: 0
        Phrase: "\u0410 \u0442\u043E\u0447\u043D\u0435\u0435 \u0432 \u043E\u0442\u0446\u0435."
