      - Character: 0
        Emotion: 0
        Phrase: "\u0418\u0437\u0432\u0438\u043D\u0438, \u043D\u043E \u0435\u0441\u043B\u0438
          \u0442\u0430\u043A \u0431\u0443\u0434\u0435\u0442 \u043F\u0440\u043E\u0434\u043E\u043B\u0436\u0430\u0442\u044C\u0441\u044F
          \u0432 \u0442\u043E\u043C \u0436\u0435 \u0434\u0443\u0445\u0435, \u043C\u044B
          \u0431\u0443\u0434\u0435\u043C \u0432\u044B\u043D\u0443\u0436\u0434\u0435\u043D\u044B
          \u043E\u0442 \u043D\u0435\u0451 \u0438\u0437\u0431\u0430\u0432\u0438\u0442\u044C\u0441\u044F\xBB."
      - Character: 0
        Emotion: 1
        Phrase: "\u041E\u0442 \u043A\u043E\u0433\u043E \u043E\u043D\u0438 \u0445\u043E\u0442\u044F\u0442
          \xAB\u0438\u0437\u0431\u0430\u0432\u0438\u0442\u044C\u0441\u044F\xBB?"
      - Character: 0
        Emotion: 0
        Phrase: "\u0418 \u043F\u0440\u0438\u0447\u0451\u043C \u0442\u0443\u0442 \u043C\u0430\u043C\u0430?"
      - Character: 0
        Emotion: 0
        Phrase: "\u0411\u043E\u044E\u0441\u044C, \u0443 \u043D\u0430\u0441 \u0441
          \u043D\u0435\u0439 \u0441\u043E\u0441\u0442\u043E\u0438\u0442\u0441\u044F
          \u043D\u0435 \u043E\u0447\u0435\u043D\u044C \u043F\u0440\u0438\u044F\u0442\u043D\u044B\u0439
          \u0440\u0430\u0437\u0433\u043E\u0432\u043E\u0440."
      - Character: 3
        Emotion: 0
        Phrase: "\u041C\u0430\u0433\u0434\u0430\u043B\u0435\u043D\u0430, \u044D\u0442\u043E
          \u0442\u0435\u0431\u044F \u043D\u0435 \u043A\u0430\u0441\u0430\u0435\u0442\u0441\u044F."
      - Character: 0
        Emotion: 0
        Phrase: "\u041F\u043E\u0433\u043E\u0434\u0438, \u0442\u0443\u0442 \u0435\u0449\u0451
